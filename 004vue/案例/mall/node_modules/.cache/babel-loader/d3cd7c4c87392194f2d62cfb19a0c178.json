is.clist[0].id\r\n    //  this.getlist(id,1)\r\n    this.clist.forEach(async (item,index)=>{\r\n    item.plist= await this.getlist(item.id,1)\r\n    // console.log(item.plist)\r\n    this.$forceUpdate()\r\n    \r\n    })\r\n    },\r\n\r\n    methods: {\r\n        async getlist(cid,page){\r\n            let res = await axios.get(api.GoodsList,{params:{categoryId:cid,page,size:20}})\r\n            // console.log(res)\r\n            return res.data.data\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.price{\r\n    color :red;\r\n}\r\n.van-ellipsis{\r\n    width: 100%;\r\n    padding: 0 10px;\r\n    margin: 0px;\r\n}\r\n.van-grid-item{\r\n    overflow: hidden;\r\n    box-sizing: border-box;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}