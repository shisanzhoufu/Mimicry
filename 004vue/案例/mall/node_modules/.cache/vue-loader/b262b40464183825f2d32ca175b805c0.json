\r\n        <div class=\"info\">\r\n            <span>30天无忧退货</span>\r\n            <span>48小时快速退款</span>\r\n            <span>满88元免邮费</span>\r\n        </div>\r\n        <div>\r\n        <h3>{{this.data.info.name}}</h3>\r\n        <h6 class=\"brief\">{{data.info.goods_brief}}</h6>\r\n        <P class=\"price\">￥{{data.info.retail_price}}</P>\r\n        </div>\r\n\r\n        <!-- 单元格 -->\r\n        <van-cell title=\"请选择规格数量\" is-link />\r\n       \r\n       <!-- 商品参数 -->\r\n        <div class=\"proParams\">\r\n            <h3>商品参数</h3>\r\n            <div class=\"proItem\" v-for=\"(item1,index1) in this.attribute\" :key=\"index1\">\r\n                <span class=\"title\">{{item1.name}}</span>\r\n                <span class=\"value\">{{item1.value}}</span>\r\n        </div>\r\n        </div>\r\n        <!-- 商品详情 -->\r\n        <div v-html=\"data.info.goods_desc\" class=\"desc\"></div>\r\n\r\n        <!-- 商品导航 -->\r\n        <van-goods-action>\r\n            <van-goods-action-icon icon=\"cart-o\" text=\"购物车\"/>\r\n            <van-goods-action-icon icon=\"shop-o\" text=\"店铺\" />\r\n            <van-goods-action-button color=\"#be99ff\" text=\"加入购物车\" type=\"warning\" />\r\n            <van-goods-action-button color=\"#7232dd\" text=\"立即购买\" />\r\n        </van-goods-action>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport api from '../assets/config/api'\r\nexport default {\r\n    props: ['id'],\r\n    data () {\r\n        return {\r\n           data:{} \r\n        }\r\n    },\r\n    methods: {\r\n        onClickLeft(){\r\n            this.$router.go(-1)\r\n        }\r\n    },\r\n    async created () {\r\n        let res=await axios.get(api.GoodsDetail,{params:{id:this.id}})\r\n        console.log(res)\r\n        this.data=res.data.data\r\n        this.info = this.data.info;\r\n        this.attribute = this.data.attribute;\r\n    },\r\n    computed: {\r\n        images(){\r\n            if(this.data.gallery){\r\n                return this.data.gallery\r\n            }else{\r\n                return []\r\n            }\r\n        }\r\n    } \r\n}\r\n</script>\r\n\r\n<style>\r\n  .swiperimg{\r\n        width: 100%;\r\n    }\r\n    .info{\r\n        display: flex;\r\n        justify-content: space-around;\r\n        color: #999;\r\n        font-size: 12px;\r\n        background-color: #efefef;\r\n        height: 24px;\r\n        line-height: 24px;\r\n    }\r\n    .brief{\r\n        color: #ccc;\r\n    }\r\n    .price{\r\n        color: red;\r\n    }\r\n    .proItem{\r\n        width: 90%;\r\n        margin: 0 auto;\r\n        display: flex;\r\n        justify-content: space-between;\r\n        height: 24px;\r\n        color: #999;\r\n        font-size: 12px;\r\n        background: #efefef;\r\n        line-height: 24px;\r\n        text-align: left; \r\n    }\r\n    .title{\r\n        width:45px;\r\n        padding: 0 10px;\r\n        border-right: 1px solid #ccc;\r\n    }\r\n    .value{\r\n        width: 260px;\r\n        overflow: hidden;\r\n        text-overflow: ellipsis;\r\n        white-space: nowrap;\r\n    }\r\n    .desc{\r\n        widows: 100%;\r\n        \r\n    }\r\n    .desc\r\n    img{\r\n        width: 100%;\r\n\r\n    }\r\n    .desc p{\r\n        display: flex;\r\n        margin: 0px;\r\n        padding: 0px;\r\n    }\r\n\r\n</style>"]}]}